[
  {
    "name": "via-ferrata",
    "base": "https://www.viaferrata-fr.net/",
    "home": "via-ferrata.html",
    "selectors": [".tr1 a", ".tr2 a"],
    "properties": {
      "name": {
        "selector": "h1 a:last-of-type"
      },
      "latitude": {
        "selector": "#latitude"
      },
      "longitude": {
        "selector": "#longitude"
      },
      "infos": [
        {
          "id": "starting_altitude",
          "label": "altitude de départ",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(1) + td",
          "index": 0,
          "filter": {
            "type": "range"
          }
        },
        {
          "id": "arrival_altitude",
          "label": "altitude d'arrivée",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(1) + td",
          "index": 1,
          "filter": {
            "type": "range"
          }
        },
        {
          "id": "drop",
          "label": "dénivelé",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(1) + td",
          "index": 2,
          "filter": {
            "type": "range"
          }
        },
        {
          "id": "difficulty",
          "label": "difficulté",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(3) + td",
          "index": 0,
          "filter": {
            "type": "selectmultiple",
            "options": [
              "F-",
              "F",
              "F+",
              "AD-",
              "AD",
              "AD+",
              "D-",
              "D",
              "D+",
              "TD-",
              "TD",
              "TD+",
              "ED-",
              "ED",
              "ED+"
            ]
          }
        },
        {
          "id": "length",
          "label": "longueur",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(3) + td",
          "index": 1,
          "filter": {
            "type": "range"
          }
        },
        {
          "id": "price",
          "label": "prix",
          "selector": "tr:has(td.topoTitre) > td.topoTitre:nth-child(3) + td",
          "index": 2
        },
        {
          "id": "description",
          "label": "description",
          "selector": "div.topoTexte",
          "strip": false
        }
      ]
    }
  }
]
